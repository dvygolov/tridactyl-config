bindurl mail.google.com --mode=ignore J tabprev
bindurl mail.google.com --mode=ignore K tabnext
bindurl mail.google.com --mode=ignore b fillcmdline tab
bindurl mail.google.com --mode=ignore B fillcmdline taball
